<form name="contact" method="POST" netlify-honeypot="bot-field" data-netlify="true" action="contact/succes">
    <p class="hidden">
        <label>Ne remplissez pas ce formulaire si vous êtes humain<input name="bot-field" /></label>
    </p>
    <p>
        <label>Quel est votre nom ou celui de votre entreprise ?<input type="text" name="nom" /></label>
    </p>
    <p>
        <label>Quel est votre email ?<input type="email" name="mail" /></label>
    </p>
    <p>
        <label>C'est à quel sujet ? <em>(facultatif)</em><input type="text" name="objet" /></label>
    </p>
    <p>
        <label>Votre message<textarea name="message"></textarea></label>
    </p>
    <p>
        <button type="submit">Envoyer</button>
    </p>
</form>

